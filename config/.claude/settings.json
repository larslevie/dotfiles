{
  "awsAuthRefresh": "aws sso login --profile witco-prd",
  "enabledPlugins": {
    "code-review@claude-plugins-official": true,
    "greptile@claude-plugins-official": true,
    "ralph-loop@claude-plugins-official": true,
    "security-guidance@claude-plugins-official": true,
    "typescript-lsp@claude-plugins-official": true
  },
  "env": {
    "ANTHROPIC_MODEL": "arn:aws:bedrock:us-east-1:558529356944:application-inference-profile/dqsyhuye1t1g",
    "ANTHROPIC_SMALL_FAST_MODEL": "arn:aws:bedrock:us-east-1:558529356944:application-inference-profile/z7ob6jgvgi19",
    "AWS_PROFILE": "witco-mgt-platform-developer-claude",
    "AWS_REGION": "us-east-1",
    "CLAUDE_CODE_MAX_OUTPUT_TOKENS": "16384",
    "CLAUDE_CODE_USE_BEDROCK": "1",
    "MAX_THINKING_TOKENS": "2048"
  },
  "permissions": {
    "allow": [
      "Bash(curl:*)",
      "Bash(docker:*)",
      "Bash(gh pr create:*)",
      "Bash(gh:*)",
      "Bash(git:*)",
      "Bash(grep:*)",
      "Bash(just:*)",
      "Bash(ls:*)",
      "Bash(mkdir:*)",
      "Bash(mv:*)",
      "Bash(pkill:*)",
      "Bash(pnpm:*)",
      "Bash(rm:*)",
      "Bash(rmdir:*)",
      "Bash(true)",
      "Edit(*)",
      "Glob(*)",
      "Grep(*)",
      "LS(*)",
      "mcp__github__add_issue_comment",
      "mcp__github__add_pull_request_review_comment_to_pending_review",
      "mcp__github__create_pending_pull_request_review",
      "mcp__github__create_pull_request",
      "mcp__github__get_pull_request_comments",
      "mcp__github__get_pull_request_reviews",
      "mcp__github__merge_pull_request",
      "mcp__github__submit_pending_pull_request_review",
      "mcp__linear__get_issue",
      "mcp__linear__list_issues",
      "MultiEdit(*)",
      "WebFetch(domain:*)",
      "WebFetch(domain:github.com)"
    ],
    "defaultMode": "acceptEdits"
  }
}